{
  "name": "botc-script-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "node setup/generate-script-schema.ts && node setup/download-role-icons.ts && node setup/generate-edition-scripts.ts && node setup/generate-official-character-ids.ts",
    "validate": "node setup/ensure-characters-in-metadata.ts",
    "dev": "vite",
    "build": "vite build",
    "full-build": "npm run setup && npm run build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.6.1",
    "json-schema-to-typescript": "^15.0.4",
    "p-queue": "^8.1.1",
    "svelte": "^5.39.7",
    "svelte-check": "^4.3.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@thisux/sveltednd": "^0.0.20",
    "nanoid": "^5.1.6",
    "svelte-codicons": "^0.16.0",
    "zod": "^4.1.11"
  }
}
